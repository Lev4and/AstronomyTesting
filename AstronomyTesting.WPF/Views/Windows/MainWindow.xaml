<Window
    x:Class="AstronomyTesting.WPF.Views.Windows.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:AstronomyTesting.WPF.Views.Windows"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:mdt="http://materialdesigninxaml.net/winfx/xaml/themes"
    Title="Тестирование по астрономии"
    Width="800"
    Height="600"
    Background="#CDDC39"
    Content="{Binding PageSource}"
    DataContext="{Binding MainWindowViewModel, Source={StaticResource ViewModelLocator}}"
    Icon="/AstronomyTesting.WPF;component/Resources/Images/Icons/Icon.ico"
    ResizeMode="NoResize"
    mc:Ignorable="d" />
